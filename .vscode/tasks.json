{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build",
			"type": "shell",
			"command": "make",
            "args": ["-j8"],
			"options": {"cwd": "${workspaceRoot}/f429-RMSv3-autogen"},
			"group": 
            {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": ["$gcc"]
		},
		{
			"label": "Clean",
			"type": "shell",
			"command": "make",
            "args": ["clean"],
			"options": {"cwd": "${workspaceRoot}/f429-RMSv3-autogen"},
			"group": 
            {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": ["$gcc"]
		},
		{
			"label": "Jflash",
			"type": "shell",
			"command": "make",
            "args": ["-j8", "jflash"],
			"options": {"cwd": "${workspaceRoot}/f429-RMSv3-autogen"},
			"group": 
			{
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": ["$gcc"] 
		}
	]
}